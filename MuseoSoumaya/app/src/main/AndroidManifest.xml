<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MuseoSoumaya"
        tools:targetApi="31">
        <activity
            android:name=".VistaPerfil"
            android:exported="false"
            android:label="@string/title_activity_vista_perfil"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VistaInicioAdmin"
            android:exported="false"
            android:label="@string/title_activity_vista_inicio_admin"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VistaBoletos"
            android:exported="false"
            android:label="@string/title_activity_vista_boletos"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VistaObrasdeArte"
            android:exported="false"
            android:label="@string/title_activity_vista_obrasde_arte"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VistaInicio"
            android:exported="false"
            android:label="@string/title_activity_vista_inicio"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VistaCrearCuenta"
            android:exported="false"
            android:label="@string/title_activity_vista_crear_cuenta"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".VentanaLogin"
            android:exported="false"
            android:label="@string/title_activity_ventana_login"
            android:theme="@style/Theme.MuseoSoumaya" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MuseoSoumaya">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>